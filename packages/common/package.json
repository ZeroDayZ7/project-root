{
  "name": "@neo/common",
  "version": "1.0.0",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build:prod": "sh scripts/build-prod.sh",
    "tsc": "tsc",
    "tsup": "tsup src/index.ts --dts --dts-resolve --format esm,cjs",
    "dev": "tsup src/index.ts --dts --dts-resolve --format esm,cjs --watch"
