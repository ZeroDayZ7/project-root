// Gateway - prod
{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "./dist",
    "noEmit": false,
    "composite": false,
    "incremental": false,
    "sourceMap": false,
    "declaration": false,
    "removeComments": true,
    "paths": {
      "@/*": ["./*"],
      "@/config/*": ["./src/config/*"],
      "@/utils/*": ["./src/utils/*"]
    }
  },
  "include": ["src/**/*"]
}
