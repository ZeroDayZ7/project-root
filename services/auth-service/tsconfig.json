{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "./dist",
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "composite": false,
    "incremental": false,
    "baseUrl": "./src",
    "paths": {
      "@/controllers/*": ["./controllers/*"],
      "@/routes/*": ["./routes/*"],
    }
  },
  "include": ["src/**/*"],
}